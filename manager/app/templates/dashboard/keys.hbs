<header>
  {{link-to "New Access Key" "dashboard.keys.new" class="button"}}
</header>

<table>
  <tr>
    <th>Integration</th>
    <th>Kind</th>
    <th>Token</th>
  </tr>
  {{#each key in this}}
    <tr>
      <td>{{key.label}}</td>
      <td>{{key.token}}</td>
      <td>{{key.type}}</td>
    </tr>
  {{else}}
    <tr class="empty">
      <td colspan="3"><em>You have not created any access keys.</em></td>
    </tr>
  {{/each}}
</table>
